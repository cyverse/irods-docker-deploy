amqp_config:
  url: $RABBITMQ_URL
  exchange: $RABBITMQ_IRODS_EXCHANGE

bisque_config:
  url: $BISQUE_URL
  admin_username: $BISQUE_ADMIN_USER
  admin_password: $BISQUE_ADMIN_PASSWORD
  irods_base_url: $BISQUE_IRODS_BASE_URL
  irods_mount_path: $BISQUE_IRODS_MOUNT_PATH

irods_config:
  host: $IRODS_HOST
  port: $IRODS_ZONE_PORT
  zone: $IRODS_ZONE_NAME
  admin_username: $IRODS_ADMIN_USER
  admin_password: $IRODS_ADMIN_PASSWORD

debug: true